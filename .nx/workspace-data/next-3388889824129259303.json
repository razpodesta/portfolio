{
  "5924255676269205726": {
    "build": {
      "command": "next build",
      "options": {
        "cwd": "apps/portfolio-web",
        "tty": false
      },
      "dependsOn": [
        "^build"
      ],
      "cache": true,
      "inputs": [
        "default",
        "^production",
        {
          "externalDependencies": [
            "next"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}/apps/portfolio-web/.next/!(cache)/**/*",
        "{workspaceRoot}/apps/portfolio-web/.next/!(cache)"
      ],
      "syncGenerators": [
        "@nx/js:typescript-sync"
      ]
    },
    "dev": {
      "continuous": true,
      "command": "next dev",
      "options": {
        "cwd": "apps/portfolio-web"
      },
      "syncGenerators": [
        "@nx/js:typescript-sync"
      ]
    },
    "start": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/portfolio-web"
      },
      "dependsOn": [
        "build"
      ]
    },
    "serve-static": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/portfolio-web"
      },
      "dependsOn": [
        "build"
      ]
    },
    "build-deps": {
      "dependsOn": [
        "^build"
      ]
    },
    "watch-deps": {
      "continuous": true,
      "dependsOn": [
        "build-deps"
      ],
      "command": "pnpm exec nx watch --projects portfolio-web --includeDependentProjects -- pnpm exec nx build-deps portfolio-web"
    }
  },
